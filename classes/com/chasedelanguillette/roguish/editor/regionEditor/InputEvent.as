package com.chasedelanguillette.roguish.editor.regionEditor{	import flash.events.Event;	public class InputEvent extends Event	{		public static const DRAG_SELECTION_INIT:String		= "onDragSelectionInit";		public static const DRAG_SELECTION_COMPLETE:String		= "onDragSelectionComplete";				private var _params:Object;				public function InputEvent(type:String):void		{			super(type);		};				public function set params(ob:Object):void		{			_params = ob;		};				public function get params():Object		{			return _params;		}	}}